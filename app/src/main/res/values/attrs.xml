<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="InputView">
        <attr name="android:label" />
        <attr name="android:text" />
        <attr name="android:inputType" />
        <attr name="android:imeOptions" />
        <attr name="android:required" />
        <attr name="readonly" format="boolean" />
        <attr name="multiline" format="boolean" />
        <attr name="format" format="string" />
    </declare-styleable>

    <declare-styleable name="LazyView">
        <attr name="lazy_background" format="reference" />
        <attr name="lazy_height" format="dimension" />
        <attr name="lazy_alpha" format="float" />
        <attr name="lazy_light_alpha" format="float" />
        <attr name="lazy_light_animation_duration" format="integer" />
    </declare-styleable>

</resources>