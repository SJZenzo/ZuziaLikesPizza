<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    tools:layout_gravity="center">

    <ImageView
        android:id="@+id/layout_empty_pizzas_list_wood_lab_end"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:scaleType="fitXY"
        android:src="@drawable/wood_lab_end_image"
        app:layout_constraintBottom_toBottomOf="@+id/layout_empty_pizzas_list_wood_lab_start_pointer"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="@+id/layout_empty_pizzas_list_wood_lab_start_pointer"
        tools:ignore="ContentDescription" />

    <ImageView
        android:id="@+id/layout_empty_pizzas_list_wood_lab_middle"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:scaleType="fitXY"
        android:src="@drawable/wood_lab_middle_image"
        app:layout_constraintBottom_toBottomOf="@+id/layout_empty_pizzas_list_wood_lab_start_pointer"
        app:layout_constraintEnd_toStartOf="@+id/layout_empty_pizzas_list_wood_lab_end_image_pointer"
        app:layout_constraintStart_toEndOf="@+id/layout_empty_pizzas_list_wood_lab_start_pointer"
        app:layout_constraintTop_toTopOf="@+id/layout_empty_pizzas_list_wood_lab_start_pointer"
        tools:ignore="ContentDescription" />

    <ImageView
        android:id="@+id/layout_empty_pizzas_list_wood_lab_start"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginVertical="@dimen/middle_space"
        android:adjustViewBounds="true"
        android:src="@drawable/wood_lab_start_image"
        app:layout_constraintBottom_toTopOf="@+id/fragment_pizzas_list_empty_list_message"
        app:layout_constraintStart_toStartOf="parent"
        tools:ignore="ContentDescription" />

    <View
        android:id="@+id/layout_empty_pizzas_list_wood_lab_start_pointer"
        android:layout_width="1dp"
        android:layout_height="1dp"
        android:layout_marginEnd="@dimen/wood_lap_overlap"
        app:layout_constraintBottom_toBottomOf="@+id/layout_empty_pizzas_list_wood_lab_start"
        app:layout_constraintEnd_toEndOf="@+id/layout_empty_pizzas_list_wood_lab_start"
        app:layout_constraintTop_toTopOf="@+id/layout_empty_pizzas_list_wood_lab_start" />

    <View
        android:id="@+id/layout_empty_pizzas_list_wood_lab_end_image_pointer"
        android:layout_width="1dp"
        android:layout_height="1dp"
        android:layout_marginStart="@dimen/wood_lap_overlap"
        app:layout_constraintBottom_toBottomOf="@+id/layout_empty_pizzas_list_wood_lab_end"
        app:layout_constraintStart_toStartOf="@+id/layout_empty_pizzas_list_wood_lab_end"
        app:layout_constraintTop_toTopOf="@+id/layout_empty_pizzas_list_wood_lab_end" />

    <TextView
        android:id="@+id/fragment_pizzas_list_empty_list_message"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginHorizontal="@dimen/big_space"
        android:fontFamily="@font/kalam"
        android:text="@string/fragment_pizzas_list_empty_list_message"
        android:textAlignment="center"
        android:textColor="@color/mahogany"
        android:textSize="32sp"
        android:lineSpacingMultiplier="0.8"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/layout_empty_pizzas_list_wood_lab_end_second"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:scaleType="fitXY"
        android:scaleX="-1"
        android:src="@drawable/wood_lab_end_image"
        app:layout_constraintBottom_toBottomOf="@+id/layout_empty_pizzas_list_wood_lab_start_pointer_second"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/layout_empty_pizzas_list_wood_lab_start_pointer_second"
        tools:ignore="ContentDescription" />

    <ImageView
        android:id="@+id/layout_empty_pizzas_list_wood_lab_middle_second"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:scaleType="fitXY"
        android:scaleX="-1"
        android:src="@drawable/wood_lab_middle_image"
        app:layout_constraintBottom_toBottomOf="@+id/layout_empty_pizzas_list_wood_lab_start_pointer_second"
        app:layout_constraintEnd_toStartOf="@+id/layout_empty_pizzas_list_wood_lab_start_pointer_second"
        app:layout_constraintStart_toEndOf="@+id/layout_empty_pizzas_list_wood_lab_end_image_pointer_second"
        app:layout_constraintTop_toTopOf="@+id/layout_empty_pizzas_list_wood_lab_start_pointer_second"
        tools:ignore="ContentDescription" />

    <ImageView
        android:id="@+id/layout_empty_pizzas_list_wood_lab_start_second"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/middle_space"
        android:adjustViewBounds="true"
        android:scaleX="-1"
        android:src="@drawable/wood_lab_start_image"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/fragment_pizzas_list_empty_list_message"
        tools:ignore="ContentDescription" />

    <View
        android:id="@+id/layout_empty_pizzas_list_wood_lab_start_pointer_second"
        android:layout_width="1dp"
        android:layout_height="1dp"
        android:layout_marginStart="@dimen/wood_lap_overlap"
        app:layout_constraintBottom_toBottomOf="@+id/layout_empty_pizzas_list_wood_lab_start_second"
        app:layout_constraintStart_toStartOf="@+id/layout_empty_pizzas_list_wood_lab_start_second"
        app:layout_constraintTop_toTopOf="@+id/layout_empty_pizzas_list_wood_lab_start_second" />

    <View
        android:id="@+id/layout_empty_pizzas_list_wood_lab_end_image_pointer_second"
        android:layout_width="1dp"
        android:layout_height="1dp"
        android:layout_marginEnd="@dimen/wood_lap_overlap"
        app:layout_constraintBottom_toBottomOf="@+id/layout_empty_pizzas_list_wood_lab_end_second"
        app:layout_constraintEnd_toEndOf="@+id/layout_empty_pizzas_list_wood_lab_end_second"
        app:layout_constraintTop_toTopOf="@+id/layout_empty_pizzas_list_wood_lab_end_second" />


</androidx.constraintlayout.widget.ConstraintLayout>